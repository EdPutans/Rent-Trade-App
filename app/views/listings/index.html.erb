<h2> This is the index page of Listings</h2>
<hr>
  <% @listings.each do |l| %>
<table>
  <tr>
      <td><a href="<%= listing_path(l) %>"><img src="<%= l.picture_url%>" style="max-width:200px;max-height:100px;"> </a> </td>
      <td><%= l.title %></td>
      <td><%= l.description %></td><br>
  </tr>
</table><hr>
  <% end %>

